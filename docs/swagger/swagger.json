{
    "swagger": "2.0",
    "info": {
        "description": "A comprehensive diagnostic tool for Docker networking issues",
        "title": "Docker Network Doctor API",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "name": "API Support",
            "url": "http://www.github.com/zebiner/docker-net-doctor",
            "email": "support@example.com"
        },
        "license": {
            "name": "Apache 2.0",
            "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
        },
        "version": "1.0"
    },
    "host": "localhost:8080",
    "basePath": "/api/v1",
    "paths": {
        "/check/{checkType}": {
            "post": {
                "description": "Creates the cobra command for running specific diagnostic checks",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "commands"
                ],
                "summary": "Create check command",
                "parameters": [
                    {
                        "enum": [
                            "dns",
                            "bridge",
                            "connectivity",
                            "ports",
                            "iptables",
                            "forwarding",
                            "mtu",
                            "overlap"
                        ],
                        "type": "string",
                        "description": "Type of check to run",
                        "name": "checkType",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/github_com_zebiner_docker-net-doctor_internal_diagnostics.Results"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "404": {
                        "description": "Check type not found",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/diagnose": {
            "post": {
                "description": "Creates the cobra command for running comprehensive diagnostics",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "commands"
                ],
                "summary": "Create diagnose command",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Filter diagnostics to specific container",
                        "name": "container",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Filter diagnostics to specific network",
                        "name": "network",
                        "in": "query"
                    },
                    {
                        "type": "boolean",
                        "default": true,
                        "description": "Run checks in parallel",
                        "name": "parallel",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/github_com_zebiner_docker-net-doctor_internal_diagnostics.Results"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/report": {
            "post": {
                "description": "Creates the cobra command for generating detailed diagnostic reports",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "commands"
                ],
                "summary": "Create report command",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Save report to file",
                        "name": "output-file",
                        "in": "query"
                    },
                    {
                        "type": "boolean",
                        "description": "Include system configuration",
                        "name": "include-system",
                        "in": "query"
                    },
                    {
                        "type": "boolean",
                        "description": "Include container logs",
                        "name": "include-logs",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Report generated successfully",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "github_com_zebiner_docker-net-doctor_internal_diagnostics.CheckResult": {
            "type": "object",
            "properties": {
                "checkName": {
                    "type": "string"
                },
                "details": {
                    "description": "Additional diagnostic data",
                    "type": "object",
                    "additionalProperties": true
                },
                "duration": {
                    "description": "New: execution time",
                    "allOf": [
                        {
                            "$ref": "#/definitions/time.Duration"
                        }
                    ]
                },
                "message": {
                    "type": "string"
                },
                "success": {
                    "type": "boolean"
                },
                "suggestions": {
                    "description": "Suggested fixes",
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "timestamp": {
                    "type": "string"
                }
            }
        },
        "github_com_zebiner_docker-net-doctor_internal_diagnostics.ExecutionMetrics": {
            "type": "object",
            "properties": {
                "apicallsCount": {
                    "type": "integer",
                    "format": "int64"
                },
                "averageCheckTime": {
                    "$ref": "#/definitions/time.Duration"
                },
                "errorRate": {
                    "type": "number",
                    "format": "float64"
                },
                "maxCheckTime": {
                    "$ref": "#/definitions/time.Duration"
                },
                "memoryUsageMB": {
                    "type": "number",
                    "format": "float64"
                },
                "minCheckTime": {
                    "$ref": "#/definitions/time.Duration"
                },
                "parallelExecution": {
                    "type": "boolean"
                },
                "rateLimitHits": {
                    "type": "integer",
                    "format": "int64"
                },
                "totalDuration": {
                    "$ref": "#/definitions/time.Duration"
                },
                "workerCount": {
                    "type": "integer"
                }
            }
        },
        "github_com_zebiner_docker-net-doctor_internal_diagnostics.Results": {
            "type": "object",
            "properties": {
                "checks": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/github_com_zebiner_docker-net-doctor_internal_diagnostics.CheckResult"
                    }
                },
                "duration": {
                    "$ref": "#/definitions/time.Duration"
                },
                "metrics": {
                    "description": "New: performance metrics",
                    "allOf": [
                        {
                            "$ref": "#/definitions/github_com_zebiner_docker-net-doctor_internal_diagnostics.ExecutionMetrics"
                        }
                    ]
                },
                "summary": {
                    "$ref": "#/definitions/github_com_zebiner_docker-net-doctor_internal_diagnostics.Summary"
                }
            }
        },
        "github_com_zebiner_docker-net-doctor_internal_diagnostics.Summary": {
            "type": "object",
            "properties": {
                "criticalIssues": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "failedChecks": {
                    "type": "integer"
                },
                "passedChecks": {
                    "type": "integer"
                },
                "totalChecks": {
                    "type": "integer"
                },
                "warningChecks": {
                    "type": "integer"
                }
            }
        },
        "time.Duration": {
            "type": "integer",
            "format": "int64",
            "enum": [
                -9223372036854775808,
                9223372036854775807,
                1,
                1000,
                1000000,
                1000000000,
                60000000000,
                3600000000000,
                1,
                1000,
                1000000,
                1000000000,
                60000000000,
                3600000000000
            ],
            "x-enum-varnames": [
                "minDuration",
                "maxDuration",
                "Nanosecond",
                "Microsecond",
                "Millisecond",
                "Second",
                "Minute",
                "Hour",
                "Nanosecond",
                "Microsecond",
                "Millisecond",
                "Second",
                "Minute",
                "Hour"
            ]
        }
    }
}